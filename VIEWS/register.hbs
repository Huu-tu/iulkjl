{{> header}}
<div class="custom-container">
    <div class="jumbotron">
        <h3 class="text-center text-primary">Registration Form</h3>
        <br>
        <form method="post" id="registration" action="">
            <div class="form-group">
                <label for="username">UserName<span class="text-danger">*</span></label>
                <input type="text" class="form-control" id="username" name="username" placeholder="Enter Your UserName" value="{{username}}">
            </div>
            <div class="form-group">
                <label for="password">PassWord</label>
                <input type="text" class="form-control" id="password" name="password" placeholder="Enter Your PassWord" value="{{password}}">
            </div>
            <div class="form-group">
                <label for="email">Email<span class="text-danger">*</span></label>
                <input type="text" class="form-control" id="email" name="email" placeholder="Enter Your Email" value="{{email}}">
            </div>
            <div class="form-group">
                <label for="fullName">Full Name<span class="text-danger">*</span></label>
                <input type="text" class="form-control" name="fullName" id="fullName" placeholder="Enter your FullName" value="{{fullName}}">
            </div>
            <div class="form-group">
                <label for="toiecScore">ToiecScore<span class="text-danger">*</span></label>
                <input type="text" class="form-control" name="toiecScore" id="toiecScore" placeholder="Enter your ToiecScore" value="{{toiecScore}}">
            </div>
            <div class="form-group">
                <label for="address">Address<span class="text-danger">*</span></label>
                <input type="text" class="form-control" name="address" id="address" placeholder="Enter Your Address" value="{{address}}">
            </div>
            <div class="form-group">
                <label for="term">Term<span class="text-danger">*</span></label>
                <input type="password" class="form-control" name="term" id="term" placeholder="Enter Same as Term" value="{{term}}">
            </div>
            <div class="form-group">
                <label for="role">Role<span class="text-danger">*</span></label>
                <input type="password" class="form-control" name="role" id="role" placeholder="Enter Same as Role" value="{{roles}}">
            </div>
            <div class="form-group">
                <input type="submit" value="Register" class="btn btn-info btn-block">
            </div>
            <div class="form-group">
                <div class="row">
                    <div class="col-md-8">
                        Already user? Login <a href="/api/auth/signin">here</a>
                    </div>
                </div>
            </div>
        </form>
    </div>
</div>
{{> footer}}